{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "startTime": {
      "type": "datetime"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "events"
    },
    "endTime": {
      "type": "datetime"
    },
    "arrival": {
      "displayName": "addresse",
      "type": "component",
      "repeatable": false,
      "component": "events.addresse"
    },
    "description": {
      "type": "component",
      "repeatable": false,
      "component": "events.description"
    },
    "repeat": {
      "type": "component",
      "repeatable": false,
      "component": "events.cycle-event"
    },
    "event_assignment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-assignment.event-assignment",
      "inversedBy": "events"
    },
    "event_state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-state.event-state",
      "inversedBy": "events"
    },
    "event_exceptions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-exception.event-exception",
      "mappedBy": "event"
    },
    "participantRestriction": {
      "type": "enumeration",
      "enum": [
        "Diese Veranstaltung ist ausschließlich für Legionäre",
        "Diese Veranstaltung ist ausschließlich für Hilfslegionäre",
        "Diese Veranstaltung ist ausschließlich für Legionäre und Hilfslegionäre",
        "Diese Veranstaltung ist für alle"
      ],
      "required": true
    }
  }
}
